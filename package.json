{
	"name": "bauhaus",
	"version": "0.0.0",
	"license": "MIT",
	"private": true,
	"workspaces": [
		"packages/*",
		"app"
	],
	"scripts": {
		"start": "yarn workspace Bauhaus run start",
		"build-insee": "yarn workspaces run build-insee",
		"test:coverage": "yarn workspaces run test:coverage",
		"build-storybook": "yarn workspaces run build-storybook",
		"pre-push": "yarn workspaces run test:coverage && yarn workspaces run lint"
	},
	"resolutions": {
		"@inseefr/wilco": "0.0.5"
	},
	"devDependencies": {
		"@babel/core": "7.10.2",
		"@babel/preset-env": "7.10.2",
		"@storybook/addon-actions": "5.3.19",
		"@storybook/addon-knobs": "5.3.19",
		"@storybook/addon-links": "5.3.19",
		"@storybook/addons": "5.3.19",
		"@storybook/react": "5.3.19",
		"@testing-library/jest-dom": "5.11.0",
		"@testing-library/react": "10.4.3",
		"@testing-library/user-event": "12.0.11",
		"@types/jest": "25.2.3",
		"babel-eslint": "^10.1.0",
		"babel-jest": "26.0.1",
		"babel-loader": "8.1.0",
		"eslint": "7.2.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-config-react-app": "5.2.1",
		"eslint-plugin-flowtype": "^5.1.0",
		"eslint-plugin-import": "2.21.1",
		"eslint-plugin-jest-dom": "^2.1.0",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-react": "7.20.0",
		"eslint-plugin-react-hooks": "4.0.4",
		"eslint-plugin-testing-library": "^3.2.0",
		"husky": "3.0.2",
		"jest": "26.0.1",
		"jest-localstorage-mock": "2.4.2",
		"lcov-result-merger": "3.1.0",
		"webpack": "4.43.0"
	},
	"dependencies": {
		"prop-types": "15.7.2",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-redux": "7.2.0",
		"react-router-dom": "5.2.0",
		"redux": "4.0.5"
	},
	"husky": {
		"hooks": {
			"pre-push": "yarn pre-push"
		}
	},
	"prettier": {
		"bracketSpacing": true,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5",
		"useTabs": true
	}
}
